<h1>New Song</h1>

<%= form_for @song do |f| %>
    <label>Name:
        <%= f.text_field :name %>
    </label>

    <label>Artist ID:
        <%= f.text_field :artist_id %>
    </label>

    <label>Genre ID:
        <%= f.text_field :genre_id %>
    </label>

    <%= f.submit %>
<% end %>